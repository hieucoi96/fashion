{
  "expo": {
    "name": "fasion",
    "slug": "fasion",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon_fasions_1.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.teklabsvn.fasion",
      "buildNumber": "1.0.4",
      "supportsTablet": false
    },
    "android": {
      "package": "com.teklabsvn.fasion",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_fasions_1.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["NOTIFICATIONS"],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
